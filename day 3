1.segregation of 0,1,2
    #include<stdio.h>
    int main() {
    int n,count0=0,count1=0,count2=0;
    //size of array
    scanf("%d",&n);
    //array initialization
    int arr[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
        if(arr[i]==0)
        count0++;
        else if(arr[i]==1)
        count1++;
        else
        count2++;
    }
    for(int i=0;i<count0;i++)
    {
        printf("0");
    }
    for(int i=0;i<count1;i++)
    {
        printf("1");
    }
    for(int i=0;i<count2;i++)
    {
        printf("2");
    }
    return 0;
}
or
#include<stdio.h>
    int main() {
    int n,temp;
    //size of array
    scanf("%d",&n);
    //array initialization
    int arr[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
      for(int i=0;i<n;i++)
    {
        for(int j=i+1;j<n;j++)
        {
            if(arr[j]<arr[i])
            {
            temp=arr[j];
            arr[j]=arr[i];
            arr[i]=temp;
            }
        }
    }
    for(int i=0;i<n;i++)
    {
        printf("%d\t",arr[i]);
    }
    return 0;
2.leader
#include<stdio.h>
    int main() {
    int n;
    //size of array
    scanf("%d",&n);
    //array initialization
    int arr[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(int i=0;i<n-1;i++)
    {
        int count=0;
        for(int j=i+1;j<n;j++)
        {
        if(arr[i]>arr[j])
        count++;
        }
        if(count==n-i-1)
        {
        printf("%d\t",arr[i]);
       
        }
        
    }
    printf("%d",arr[n-1]);
    return 0;
}
